@model Identity.CustomIdentityDB.ViewModels.RegisterAuthenticatorViewModel

<div class="row">
    <div class="col-md-8 offset-md-2">
        <h1 class="text-center">Register Authenticator</h1>

        <form asp-controller="Account" asp-action="RegisterAuthenticator" method="POST">
            <!-- AuthenticatorKey -->
            <div class="form-group">
                <p>Your authentication key is : @Model.AuthenticatorKey</p>
            </div>

            <!-- Code -->
            <div class="form-group">
                <label asp-for="Code" class="col-md-2">Code</label>
                <div class="col-md-12">
                    <input asp-for="Code" type="text" class="form-control" />
                </div>
            </div>

            <div class="form-group">
                <div class="col-md-12">
                    <button type="submit" class="btn btn-primary">Submit</button>
                </div>
            </div>

            <div asp-validation-summary="All"></div>
        </form>
    </div>
</div>